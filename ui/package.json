{
  "name": "angkor-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "npx jest --coverage",
    "test-debug": "npx jest --detectOpenHandles",
    "test-clear": "npx jest --clear-cache && echo you may also want to rm yarn.lock and reinstall",
    "test-chrome": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "mock": "json-server --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js",
    "mock:delay": "json-server --delay 3000 --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@angular/youtube-player": "^12.0.0",
    "angular-tag-cloud-module": "^6.0.0",
    "core-js": "^3.13.1",
    "date-fns": "^2.16.1",
    "lodash-es": "^4.17.20",
    "mapbox-gl": "^2.1.1",
    "ngx-date-fns": "^8.0.0",
    "ngx-logger": "^4.2.1",
    "ngx-mapbox-gl": "^6.0.4",
    "ngx-markdown": "^12.0.0",
    "ngx-stars": "^1.5.1",
    "ngx-web-storage": "^0.2.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@angular/language-service": "~12.0.1",
    "@types/jasmine": "~3.7.4",
    "@types/jasminewd2": "~2.0.9",
    "@types/jest": "^26.0.23",
    "@types/mapbox-gl": "^2.1.1",
    "@types/node": "^15.9.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^27.0.4",
    "jest-preset-angular": "^9.0.1",
    "protractor": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
