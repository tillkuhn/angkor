{
  "name": "angkor-ui",
  "version": "1.0.0",
  "@comment scripts": {
    "test": [
      "This comment is about the `test` script.",
      "It is wrapped in an array to allow line formatting",
      "@comment workaround: https://stackoverflow.com/a/58385464/4292075 (since JSON does not allow comments)"
    ],
    "start": "This script starts the server using 'ng serve'",
    "mock": "This script runs the json-server for mocks"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:debug": "npx jest --detectOpenHandles",
    "test:clear": "npx jest --clear-cache && echo you may also want to rm yarn.lock and reinstall",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "mock": "json-server --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js",
    "mock:delay": "json-server --delay 3000 --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js"
  },
  "private": true,
  "//engines-comment": "Keep in sync with https://github.com/angular/angular/blob/master/package.json (appropriate tag)",
  "engines": {
    "node": "^12.20.0 || ^14.15.0 || >=16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "dependencies": {
    "@angular/animations": "~13.3.8",
    "@angular/cdk": "~13.3.7",
    "@angular/common": "~13.3.8",
    "@angular/compiler": "~13.3.8",
    "@angular/core": "~13.3.8",
    "@angular/forms": "~13.3.8",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~13.3.8",
    "@angular/platform-browser-dynamic": "~13.3.8",
    "@angular/router": "~13.3.8",
    "@angular/youtube-player": "^13.3.7",
    "@mapbox/mapbox-gl-geocoder": "^5.0.0",
    "angular-tag-cloud-module": "^13.0.0",
    "core-js": "3.22.4",
    "lodash-es": "^4.17.20",
    "mapbox-gl": "^2.8.2",
    "ngx-logger": "^5.0.9",
    "ngx-mapbox-gl": "^7.1.2",
    "ngx-markdown": "^13.1.0",
    "ngx-web-storage": "^0.2.0",
    "rxjs": "7.5.5",
    "ts-jest": "^28.0.1",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.8",
    "@angular/language-service": "~13.3.8",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^27.5.1",
    "@types/mapbox-gl": "^2.7.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "^8.15.0",
    "jest": "27",
    "jest-preset-angular": "11",
    "typescript": "4.6.2"
  }
}
