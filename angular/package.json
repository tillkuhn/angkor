{
  "name": "angkor-ui",
  "version": "1.0.0",
  "@comment scripts": {
    "test": [
      "This comment is about the `test` script.",
      "It is wrapped in an array to allow line formatting",
      "@comment workaround: https://stackoverflow.com/a/58385464/4292075 (since JSON does not allow comments)"
    ],
    "start": "This script starts the server using 'ng serve'",
    "mock": "This script runs the json-server for mocks"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:debug": "npx jest --detectOpenHandles",
    "test:clear": "npx jest --clear-cache && echo you may also want to rm yarn.lock and reinstall",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "mock": "json-server --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js",
    "mock:delay": "json-server --delay 3000 --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js"
  },
  "private": true,
  "//engines-comment": "Keep in sync with https://github.com/angular/angular/blob/master/package.json (appropriate tag)",
  "engines": {
    "node": "^12.20.0 || ^14.15.0 || >=16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "dependencies": {
    "@angular/animations": "^14.1.3",
    "@angular/cdk": "^14.1.3",
    "@angular/common": "^14.1.3",
    "@angular/compiler": "^14.1.3",
    "@angular/core": "^14.1.3",
    "@angular/forms": "^14.1.3",
    "@angular/material": "^14.1.3",
    "@angular/platform-browser": "^14.1.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@angular/router": "^14.1.3",
    "@angular/youtube-player": "^14.1.3",
    "@mapbox/mapbox-gl-geocoder": "^5.0.0",
    "angular-tag-cloud-module": "^13.0.0",
    "core-js": "3.22.4",
    "lodash-es": "^4.17.20",
    "mapbox-gl": "^2.10.0",
    "ngx-logger": "^5.0.11",
    "ngx-mapbox-gl": "^9.1.0",
    "ngx-markdown": "^14.0.1",
    "ngx-web-storage": "^0.2.0",
    "rxjs": "7.5.6",
    "ts-jest": "^28.0.8",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "^14.1.3",
    "@angular/compiler-cli": "^14.1.3",
    "@angular/language-service": "^14.1.3",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^29.0.0",
    "@types/mapbox-gl": "^2.7.5",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "jest": "28",
    "jest-preset-angular": "12",
    "typescript": "4.7.2"
  }
}
