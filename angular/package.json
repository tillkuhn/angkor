{
  "name": "angkor-ui",
  "version": "1.0.0",
  "@comment scripts": {
    "test": [
      "This comment is about the `test` script.",
      "It is wrapped in an array to allow line formatting",
      "@comment workaround: https://stackoverflow.com/a/58385464/4292075 (since JSON does not allow comments)"
    ],
    "start": "This comment is about the build script",
    "mock": "This comment is about the mock script"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:debug": "npx jest --detectOpenHandles",
    "test:clear": "npx jest --clear-cache && echo you may also want to rm yarn.lock and reinstall",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "mock": "json-server --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js",
    "mock:delay": "json-server --delay 3000 --port 8080 --watch --routes json-server/routes.json json-server/db.json --middlewares json-server/middleware.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@angular/youtube-player": "^13.1.1",
    "@mapbox/mapbox-gl-geocoder": "^4.7.4",
    "angular-tag-cloud-module": "^13.0.0",
    "core-js": "^3.19.2",
    "lodash-es": "^4.17.20",
    "mapbox-gl": "^2.6.1",
    "ngx-logger": "^5.0.3",
    "ngx-mapbox-gl": "^7.1.2",
    "ngx-markdown": "^13.0.0",
    "ngx-stars": "^1.5.1",
    "ngx-web-storage": "^0.2.0",
    "rxjs": "^7.4.0",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@angular/language-service": "~13.1.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^27.0.3",
    "@types/mapbox-gl": "^2.6.0",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "jest": "^27.4.5",
    "jest-preset-angular": "^11.0.1",
    "typescript": "4.4.4"
  }
}
